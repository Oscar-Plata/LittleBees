<header class="topBar">
  <%= link_to "/" do%>
    <%= image_tag 'logoLB.png', class: 'logo' %>
  <%end%>
  <h3>Bienvenido <%= Current.user&.nombre %></h3>
  <div>
    <% if Current.user %>
      <%= link_to "Cerrar Sesión", logout_session_path, data: {turbo: true, turbo_method: :delete} %>
    <% else %>
      <%= link_to "Iniciar Sesión", login_session_path, method: :get %>
    <% end %>
  </div>
</header>
